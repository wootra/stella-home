{
  "name": "stella-pretty",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "@material-ui/core": "^1.4.1",
    "@material-ui/icons": "^2.0.0",
    "@types/googlemaps": "^3.30.11",
    "@types/markerclustererplus": "^2.1.33",
    "ajv": "^6.5.2",
    "apigateway": "^1.0.1",
    "aws-config": "^1.2.0",
    "aws-sdk": "^2.285.1",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.2",
    "dotenv": "^6.0.0",
    "font-awesome": "^4.7.0",
    "google": "^2.1.0",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "popper.js": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-animated-css": "^1.0.4",
    "react-calendar": "^2.16.1",
    "react-datetime": "^2.15.0",
    "react-dom": "^16.4.1",
    "react-facebook-login": "^4.0.1",
    "react-google-login": "^3.2.1",
    "react-google-maps": "^9.4.5",
    "react-kakao-login": "^1.0.7",
    "react-nouislider": "^2.0.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.4",
    "react-slick": "^0.23.1",
    "react-swipeable-views": "^0.12.15",
    "recompose": "^0.28.1",
    "redux": "^4.0.0",
    "redux-promise": "^0.6.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start-all": "npm-run-all -p watch-css start-js",
    "start": "npm run start-js",
    "dev": "REACT_APP_SERVER_DEV=127.0.0.1:5000 npm run start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy-s3": "aws s3 sync build/ s3://stella-home",
    "deploy": "npm run build && npm run deploy-s3"
  },
  "devDependencies": {
    "@types/react": "^16.4.6",
    "@types/react-redux": "^6.0.4",
    "@types/redux": "^3.6.0",
    "@types/redux-promise": "^0.5.28",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0"
  }
}
